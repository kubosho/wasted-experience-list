        "@babel/preset-typescript": "^7.12.7",
        "@storybook/addon-actions": "^6.1.14",
        "@storybook/addon-essentials": "^6.1.15",
        "@storybook/addon-links": "^6.1.17",
        "@storybook/preact": "^6.1.14",
        "@types/chrome": "^0.0.129",
        "@types/uuid": "^8.3.0",
