{
    "name": "wasted-experience-list",
    "version": "1.0.0",
    "description": "This browser extension are listed a wasted experience Web site.",
    "author": "Shota Kubota <ta2@o2p.jp>",
    "license": "MIT",
    "private": true,
    "scripts": {
        "bundle_analyze": "babel ./src --out-dir ./lib --extensions '.ts,.tsx' && BUNDLE_ANALYZE=true webpack",
        "build": "babel ./src --out-dir ./lib --extensions '.ts,.tsx' && NODE_ENV=production webpack && npm run build_styles",
        "build_dev": "babel ./src --out-dir ./lib --extensions '.ts,.tsx' && NODE_ENV=development webpack && npm run build_styles",
        "build_styles": "postcss ./src/popup.css -o ./bundle/popup.bundle.css",
        "format": "prettier --config .prettierrc.js --write **/*.{js,jsx,ts,tsx}",
        "check_format": "npm run format && git diff --exit-code",
        "lint": "eslint --config .eslintrc.js --ext .ts,.tsx .",
        "test": "jest",
        "update_snapshot": "jest --updateSnapshot",
        "typecheck": "tsc --project .",
        "storybook": "start-storybook -p 9001",
        "build_storybook": "build-storybook"
    },
    "dependencies": {
        "@spectrum-css/button": "^3.0.3",
        "@spectrum-css/icon": "^3.0.24",
        "@spectrum-css/page": "^5.0.3",
        "@spectrum-css/table": "^3.0.3",
        "@spectrum-css/typography": "^3.0.2",
        "@spectrum-css/vars": "^3.0.0",
        "dayjs": "^1.10.7",
        "preact": "^10.5.13",
        "recoil": "^0.6.1",
        "tailwindcss": "^3.0.23",
        "uuid": "^8.3.2"
    },
    "devDependencies": {
        "@babel/cli": "^7.13.10",
        "@babel/core": "^7.12.17",
        "@babel/plugin-transform-react-jsx": "^7.12.17",
        "@babel/preset-env": "^7.13.15",
        "@babel/preset-typescript": "^7.14.5",
        "@storybook/addon-actions": "^6.4.19",
        "@storybook/addon-essentials": "^6.1.21",
        "@storybook/addon-links": "^6.3.5",
        "@storybook/preact": "^6.1.21",
        "@types/chrome": "^0.0.178",
        "@types/uuid": "^8.3.0",
        "@typescript-eslint/eslint-plugin": "^5.12.1",
        "@typescript-eslint/parser": "^5.12.1",
        "autoprefixer": "^10.2.4",
        "babel-loader": "^8.2.2",
        "copy-webpack-plugin": "^9.0.1",
        "eslint": "^8.10.0",
        "eslint-config-prettier": "^8.4.0",
        "eslint-plugin-import": "^2.22.1",
        "eslint-plugin-prettier": "^4.0.0",
        "eslint-plugin-react": "^7.29.2",
        "eslint-plugin-react-hooks": "^4.2.0",
        "jest": "^27.5.1",
        "mini-css-extract-plugin": "^1.4.0",
        "postcss": "^8.2.12",
        "postcss-cli": "^9.1.0",
        "postcss-import": "^14.0.0",
        "prettier": "^2.2.1",
        "typescript": "^4.3.5",
        "web-ext-types": "^3.2.1",
        "webpack": "^5.24.2",
        "webpack-bundle-analyzer": "^4.5.0",
        "webpack-cli": "^4.9.2"
    }
}
